<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Correspondence</id>
    <version>3.0.0.8</version>
    <authors>Michael L Perry</authors>
    <owners>Michael L Perry</owners>
    <description>
      Core assemblies for the Correspondence collaboration framework. Add the package Correspondence.App for source files, or Correspondence.Model for the data model.
    </description>
    <summary>
      Correspondence stores data on the mobile device while it is offline. It queues changes to be synchronized when the connection is restored. And when online, it updates in real-time.
      Configure all clients to use a common distributor. The distributor is available at http://github.com/michaellperry/correspondencedistributor.
    </summary>
    <licenseUrl>http://correspondence.codeplex.com/license</licenseUrl>
    <projectUrl>http://correspondencecloud.com/</projectUrl>
    <iconUrl>http://qedcode.com/images/logo32.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <language>en-US</language>
    <tags>occasionally-connected mobile nosql database synchronize</tags>
    <dependencies>
      <dependency id="Assisticant" version="1.1.4" />
    </dependencies>
  </metadata>

  <files>
    <file src="..\..\Portable\Correspondence\bin\Release\Correspondence.dll" target="lib\portable-net45+wp80+win80+MonoAndroid+MonoTouch" />
    <file src="..\..\Portable\Correspondence\bin\Release\Correspondence.dll" target="lib\net45" />
    <file src="..\..\Portable\Correspondence\bin\Release\Correspondence.dll" target="lib\wp80" />
    <file src="..\..\Portable\Correspondence\bin\Release\Correspondence.dll" target="lib\win80" />
    <file src="..\..\Portable\Correspondence\bin\Release\Correspondence.dll" target="lib\MonoAndroid" />
    <file src="..\..\Portable\Correspondence\bin\Release\Correspondence.dll" target="lib\MonoTouch" />

    <file src="..\..\Silverlight\Correspondence\bin\Release\Correspondence.dll" target="lib\sl5" />

    <file src="..\..\Universal\Correspondence\bin\Release\Correspondence.dll" target="lib\portable-wpa81+win81" />
    <file src="..\..\Universal\Correspondence\bin\Release\Correspondence.dll" target="lib\wpa81" />
    <file src="..\..\Universal\Correspondence\bin\Release\Correspondence.dll" target="lib\win81" />
  </files>
</package>