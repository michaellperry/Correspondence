<?xml version="1.0" encoding="utf-8"?>
<xs:schema elementFormDefault="unqualified" targetNamespace="http://correspondence.updatecontrols.com/pox/1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://correspondence.updatecontrols.com/pox/1.0">
	<xs:complexType name="FactTree">
